{
  "name": "prisma-next-auth-graphql-ably-chat",
  "version": "3.0.0",
  "description": "An fully working example of using prisma, nextjs, graphql, and ably. The project is built in typescript and has full models for profiles, chats, auth, and messages.",
  "scripts": {
    "start": "next start",
    "dev": "next",
    "build": "next build",
    "studio": "npx prisma studio",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/graphql/schema",
    "lint": "next lint",
    "postinstall": "npm run generate",
    "ts-node-c": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "prisma": {
    "seed": "yarn run ts-node-c prisma/seed.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "prisma",
    "starter",
    "fullstack",
    "auth"
  ],
  "author": "nrgapple & mattruddy",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "3.4.11",
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hqoss/monads": "^0.5.0",
    "@prisma/client": "3.0.2",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/ramda": "^0.27.44",
    "apollo-server-micro": "3.3.0",
    "framer-motion": "^4",
    "graphql": "15.5.3",
    "graphql-middleware": "6.1.6",
    "graphql-scalars": "1.10.1",
    "graphql-shield": "7.5.0",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "11.1.2",
    "next-auth": "3.29.0",
    "nexus": "1.1.0",
    "ramda": "^0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.5"
  },
  "devDependencies": {
    "@types/next-auth": "3.15.0",
    "@types/node": "16.9.1",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "husky": "5.2.0",
    "lint-staged": "11.1.2",
    "postcss": "8.3.6",
    "prettier": "2.4.0",
    "prisma": "3.0.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  }
}
